  .tools
    %h3 < tools />
    %p
      %i.toolsicons.icon-html5-alt
      %toolstext Html
    %p
      %i.toolsicons.icon-css
      %toolstext CSS
    %p
      %i.toolsicons.icon-javascript-alt
      %toolstext JavaScript
    %p
      %i.toolsicons.icon-ruby
      %toolstext Ruby
    %p
      %i.toolsicons.icon-ruby-on-rails
      %toolstext Ruby on rails
    %p
      %i.toolsicons.icon-angular
      %toolstext Angular
    %p
      %i.toolsicons.icon-sass
      %toolstext Sass
    %p
      %i.toolsicons.icon-jquery
      %toolstext jQuery
    %p
      %i.toolsicons.icon-database
      %toolstext SQL
  .project
    %h3 < projects />
    - data.projects. each do |project|
      %img(src="#{project[:projecticon]} "height="40px")
      %a.projecttitle{href: project[:url]}= project[:name]
      %br
      %projectdescription.projectdescription= project[:description]
      %br
      %projecttechnology.projectdescription= project[:Projecttechnology]
      %p

  .education
    %h3 < education />
    - data.education. each do |education|
      %img(src="#{education[:eduicon]} "height="40px")
      %edutext=education[:eduname]
      %br
      %projecttechnology.edutext= education[:eduwhere]
      %br
      %projecttechnology.edutext= education[:edutimes]
      %p

  .work
    %h3 < work />
    - data.work. each do |work|
      %img(src="#{work[:workicon]} "height="40px")
      %worktext= work[:workposition]
      %br
      %projecttechnology.worktext= work[:workemployer]
      %br
      %projecttechnology.worktext= work[:worktimes]
      %p
